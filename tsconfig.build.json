{
	"compilerOptions": {
		"target": "es6",
		"allowJs": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"module": "esnext",
		"lib": [
			"dom",
			"dom.iterable"
		],
		"esModuleInterop": true,
		"declaration": true,
		"composite": true,
		"noEmit": false,
		"jsx": "preserve",
		"emitDeclarationOnly": true,
		"baseUrl": ".",
		"paths": {
			"Aquaty_Project": [
				"./packages/projects/api/src"
			],
			"Aquaty_Project/*": [
				"./packages/projects/api/src/*"
			]
		},
		"typeRoots": [
			"node_modules/@types",
			"./types"
		]
	},
	"exclude": [
		"node_modules",
		"**/__tests__/**",
		"dist",
		"**jest**",
		"**resources.js"
	]
}
